      GET    /api/v1/otus/:id/collection_objects(.:format)                                    otus#collection_objects {:format=>:json, :id=>/\d+/}
      GET    /api/v1/otus/by_name/:name(.:format)                                             otus#by_name {:format=>:json}
      PATCH  /api/v1/observations/:id(.:format)                                               observations#update {:format=>:json, :id=>/\d+/}
      POST   /api/v1/observations(.:format)                                                   observations#create {:format=>:json}
